{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true
  },
  "ts-node": {
    "esm": true
  },
  "typingOptions": {
    "enableAutoDiscovery": true
  },
  "exclude": [
    "node_modules"
  ]
}
